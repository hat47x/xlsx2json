{
  "input-files": ["tests/data/card1.xlsx"],
  "output-dir": "tests/data/output", "inputs": ["tests/data/table1.xlsx"],
  "output_dir": "tests/data/output",
  "prefix": "json",
  "keep-empty": true,
  "log-level": "INFO",
  "containers": {
    "json.card": {
      "range": "card.range",
      "direction": "row", 
      "increment": 5,
      "items": ["customer_name", "address", "tel"],
      "labels": ["顧客名", "住所", "電話番号"]
    }
  }
}