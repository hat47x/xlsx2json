{
  "inputs": ["samples/sample.xlsx"],
  "output_dir": "samples/output-json",
  "schema": "samples/schema.json",
  "transform": [
    "json.parent=split:,|\n",
    "json.日本語！.（記／号）～=function:samples/transform.py:string_transform",
    "json.command_test=command:sort -u"
  ],
  "prefix": "json",
  "keep_empty": true,
  "log_level": "INFO",
  "containers": {
    "json.表1": {
      "range": "表1.range",
      "direction": "row",
      "items": ["列A", "列B", "列C"],
      "labels": ["列A", "列B", "列C"]
    },
    "json.リスト1": {
      "range": "リスト1.range",
      "direction": "row", 
      "items": ["aaa名称", "aaaコード", "bbb名称", "bbbコード"],
      "labels": ["aaa名称", "aaaコード", "bbb名称", "bbbコード"]
    },
    "json.ツリー1": {
      "range": "ツリー1.range",
      "direction": "row",
      "items": ["seq", "A"],
      "labels": ["seq", "A"]
    },
    "json.ツリー1.lv1.1.lv2": {
      "offset": 1,
      "items": ["seq", "B"],
      "labels": ["seq", "B"]
    },
    "json.ツリー1.lv1.1.lv2.1.lv3": {
      "offset": 1,
      "items": ["seq", "C"],
      "labels": ["seq", "C"]
    }
  }
}
