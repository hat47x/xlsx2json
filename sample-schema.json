{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CustomerData",
  "type": "object",
  "properties": {
    "parent": {
      "type": "array",
      "description": "親要素の二次元配列",
      "items": {
        "type": "array",
        "items": {
          "type": "string",
          "description": "親要素の文字列"
        }
      }
    },
    "customer": {
      "type": "object",
      "description": "顧客",
      "properties": {
        "name": {
          "type": "string",
          "description": "氏名"
        },
        "address": {
          "type": "string",
          "description": "住所"
        }
      },
      "required": ["name", "address"],
      "additionalProperties": false
    }
  },
  "required": ["customer", "parent"],
  "additionalProperties": false
}
