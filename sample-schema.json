{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Sample",
  "type": "object",
  "properties": {
    "parent": {
      "type": "array",
      "description": "親要素の3次元配列(縦×横×セル内カンマ区切り)",
      "items": {
        "type": "array",
        "description": "2次元配列(横方向セル範囲×セル内カンマ区切り)",
        "items": {
          "type": "array",
          "description": "1次元配列(セル内カンマ区切り)",
          "items": {
            "type": "string",
            "description": "最小要素"
          }
        }
      }
    },
    "customer": {
      "type": "object",
      "description": "顧客",
      "properties": {
        "name": {
          "type": "string",
          "description": "氏名"
        },
        "address": {
          "type": "string",
          "description": "住所"
        }
      },
      "required": ["name", "address"],
      "additionalProperties": false
    },
    "日本語！": {
      "type": "object",
      "properties": {
        "記号！": {
          "type": "string",
          "description": "Excelの「名前の定義」で使用できない記号をパスに含むケース"
        },
        "（記／号）～": {
          "type": "string",
          "description": "Excelの「名前の定義」で使用できない記号をパスに含むケース２"
        }
      }
    }
  },
  "required": ["customer", "parent", "日本語！"],
  "additionalProperties": false
}
